{
    "version":2,
    "builds": [
        {
           "src": "app.py",
            "use": "@vercel/python",
            "config": { "pythonVersion": "3.11" },
            "buildCommand": "pip install --upgrade pip && pip install -r requirements.txt"
        }
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}
