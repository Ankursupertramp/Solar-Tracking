{
  "version": 2,
  "builds": [
    {
      "src": "./app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "buildCommand": "pip install --upgrade setuptools wheel && pip install --upgrade -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12"
  }
}
